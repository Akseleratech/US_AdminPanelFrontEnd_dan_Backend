{
  "name": "unionspace-crm",
  "version": "1.0.0",
  "description": "UnionSpace CRM Application",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "firebase emulators:start --project demo-unionspace-crm",
    "client": "cd frontend && npm run dev",
    "install-all": "npm install && cd frontend && npm install && cd ../backend && npm install"
  },
  "devDependencies": {
    "concurrently": "^7.6.0"
  },
  "dependencies": {
    "react-router-dom": "^7.6.3"
  }
}
